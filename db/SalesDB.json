    "CustomerInsights": {
      "definition": "CREATE VIEW CustomerInsights AS\nSELECT\n    c.CustomerID,\n    c.FirstName,\n    c.LastName,\n    c.Email,\n    c.SignupDate,\n    -- Aggregate transaction metrics\n    COUNT(t.TransactionID) AS TransactionCount,\n    SUM(t.Amount) AS TotalSpending,\n    AVG(t.Amount) AS AverageTransactionAmount,\n    MAX(t.TransactionDate) AS MostRecentPurchaseDate\nFROM\n    Customers c\n    INNER JOIN Transactions t ON c.CustomerID = t.CustomerID\nGROUP BY\n    c.CustomerID,\n    c.FirstName,\n    c.LastName,\n    c.Email,\n    c.SignupDate;",
